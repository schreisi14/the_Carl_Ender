<div class="col-md-4" id="panel{{_id}}">
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3 class="panel-title" onclick="getText('{{_id}}')">{{local.name}}
				<a data-toggle="collapse" href="#coll{{_id}}" class="pull-right"><span class="glyphicon glyphicon-chevron-down" aria-hidden="true"></span></a>
				<a href="#" class="pull-left" onclick="deleteTask('{{_id}}')"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></a>
			</h3>
		</div>
		<div class="panel-collapse collapse" id="coll{{_id}}">

		<div class="panel-body">
			<ul class="list-group">
				<li class="list-group-item">{{local.place}}</li>
			</ul>

			<div id="collapse{{_id}}">

			</div>
		</div>
	</div>
		<div id="panel">
			<div class="panel-footer text-right">
				<span>
					{{dataFormat local.date}} - {{local.time}}
				</span>
			</div>
		</div>
	</div>
</div>
